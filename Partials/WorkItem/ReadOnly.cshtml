@model ReadOnlyPartial

<form>
    <h2>Selected Work Item</h2>
    <h5>@DateTime.UtcNow.ToLocalTime()</h5>
    <div class="form-group">
        <label for="Title" class="control-label"></label>
        <input id="Title" class="form-control" readonly value="@Model.Title" />
    </div>
    <div class="form-group">
        <label for="Description" class="control-label"></label>
        <input id="Description" class="form-control" readonly value="@Model.Description" />
    </div>
</form>